package {		public class JSONResponse {				public var status:String = "";		public var message:String = "";		public var error_code:Number = -1;		public var data:Object = {};			public function JSONResponse(data)		{			if (typeof(data) == "string" && SteelcaseAPI.is_json(data))			{				data = JSON.parse(data);			}			else if (typeof(data) == "string" && ! SteelcaseAPI.is_json(data))			{				data = { 					status: 'fail', 					message: data,					error_code: 24				};			}			else if (typeof(data) != "object")			{				return;			}				for (var i in data) 			{				this[i] = data[i];			}		}			public function is_ok()		{			return SteelcaseAPI.is_ok(this);		}			public function toString()		{			return JSON.stringify(this);		}	}}